  else
    gg.searchNumber("50000~100000", 16, false, 536870912, 0, -1)
    gg.getResults(210)
    gg.editAll("32465", 16)
    gg.clearResults()
  end
  gg.clearResults()
end

function DMG()
gg.clearResults()
local t = gg.getRangesList('libUE4.so$');
local KOPLAXS = {}
KOPLAXS[1] = {}
KOPLAXS[1].address = t[1].start + 0x37C1BA4
KOPLAXS[1].flags = gg.TYPE_FLOAT
KOPLAXS[1].value  = "0"
gg.setValues(KOPLAXS)
 gg.clearResults()
end

