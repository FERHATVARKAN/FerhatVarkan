function CLOSE()
  gg.alert("⚡ BYNOPANİK VİP ONLİNE GG️\n GÜLE GÜLE️")
  os.exit()
end
--closing
cs = "YOUR DEVICE ALWAYS ROOT"
while true do
  if gg.isVisible(true) then
    PUBGMH = 1
    gg.setVisible(false)
  end
  --gg.clearResults()
  if PUBGMH == 1 then
    HOME()
  end
end 
